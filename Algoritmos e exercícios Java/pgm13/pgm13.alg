algoritmo "pgm13"
// Função : Exponenciação
// Autor : Willian Carlos Gomes
// Data : 21/01/2021

// Seção de Declarações
var
v1, v2, res: real

inicio
//Atribuições de valores às variáveis
v1 <- (3)
v2 <- (2)

//Cálculo
res <- (v1 ^ v2)

//Impressão em tela
escreval("O resultado da exponenciação entre: ",v1, " e ",v2, " é: ", res)

fimalgoritmo
