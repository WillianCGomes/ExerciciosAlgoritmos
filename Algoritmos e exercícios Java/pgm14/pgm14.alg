algoritmo "pgm13"
// Função : Módulo
// Autor : Willian Carlos Gomes
// Data : 21/01/2021

// Seção de Declarações
var
v1, v2, res: real

inicio
//Atribuição dos valores
v1 <- (3)
v2 <- (2)

//cálculo do resto
res <- (v1 % v2)

//impressão em tela
escreval("O Módulo entre: ",v1, " e ",v2, " é: ", res)

fimalgoritmo
