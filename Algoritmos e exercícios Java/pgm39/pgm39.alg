// Seção de Declarações 
var
A, B,: inteiro

inicio
//Atribuições de valores às variáveis
A <- 10
B <- (A+1)
A<- (B+1)
B<-(A+1)

//Impressão em tela
escreval(A)

//Alteração da variável
A<- (B+1)

//Impressão em tela
escreval(A, B)

fimalgoritmo
